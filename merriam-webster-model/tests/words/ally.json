                "dt": [
                  [
                    "text",
                    "{bc}a {d_link|sovereign|sovereign:1} or state associated with another by treaty or league "
                  ],
                  [
                    "vis",
                    [
                      {
                        "t": "America and its {it}allies{/it}"
